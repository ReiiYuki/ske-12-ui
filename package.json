{
  "name": "ske-12-ui",
  "version": "1.0.0",
  "description": "UIKit for SKE-12 Site",
  "main": "index.js",
  "scripts": {
    "start": "npm run storybook",
    "storybook": "start-storybook -p 9001 -c ./",
    "build:demo": "build-storybook -c ./ -o docs",
    "deploy:demo": "gh-pages -d docs",
    "publish:demo": "npm run build:demo && npm run deploy:demo",
    "build:lib": "bnr build:lib"
  },
  "betterScripts": {
    "build:lib": {
      "command": "webpack --optimize-minimize",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ReiiYuki/ske-12-ui.git"
  },
  "keywords": [
    "ui",
    "ske",
    "ske12",
    "ske-12",
    "styled"
  ],
  "author": "SKE-12",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ReiiYuki/ske-12-ui/issues"
  },
  "homepage": "https://github.com/ReiiYuki/ske-12-ui#readme",
  "dependencies": {
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "styled-components": "^4.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-export-default": "^7.0.0-beta.32",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@storybook/react": "^3.4.11",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^8.0.4",
    "better-npm-run": "^0.1.1",
    "gh-pages": "^2.0.1",
    "husky": "^1.1.2",
    "lint-staged": "^7.3.0",
    "lodash-webpack-plugin": "^0.11.5"
  }
}
